# AI Interviewer Agent - Environment Configuration

# Google API (for Gemini)
GOOGLE_API_KEY=your_google_api_key_here

# Database Configuration
DATABASE_URL=sqlite:///./interview_agent.db

# Logging Configuration
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL

# Search API Configuration
SERPAPI_API_KEY=your_serpapi_key_here
SERPER_API_KEY=your_serper_key_here
DEFAULT_SEARCH_PROVIDER=serpapi  # serpapi or serper

# TTS Configuration (Kokoro)
TTS_API_URL=http://localhost:8001  # Local Kokoro TTS API endpoint
TTS_DEFAULT_VOICE=en-US-Neural2-F

# RAG Configuration
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2
VECTOR_DB_PATH=./vector_db

# Web Server Configuration
HOST=0.0.0.0
PORT=8000 